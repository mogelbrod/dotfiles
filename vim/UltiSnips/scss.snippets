extends css
priority 1

snippet ! "!important"
!important
endsnippet

snippet u "url()"
url(${1:sprite.png})$0
endsnippet

snippet display "display" b
display: ${1:block};$0
endsnippet

snippet di "display inline" b
display: inline;$0
endsnippet

snippet db "display block" b
display: block;$0
endsnippet

snippet dib "display inline-block" b
display: inline-block;$0
endsnippet

snippet dn "display none" b
display: none;$0
endsnippet

snippet fl "float left" b
float: left;$0
endsnippet

snippet fr "float right" b
float: right;$0
endsnippet

snippet fn "float none" b
float: none;$0
endsnippet

snippet cl "clear" b
clear: ${1:both};$0
endsnippet

snippet ps "position static" b
position: static;$0
endsnippet

snippet pa "position absolute" b
position: absolute;$0
endsnippet

snippet pos "position" b
position: ${1:relative};$0
endsnippet

snippet z "z-index" b
z-index: ${1:auto};$0
endsnippet

snippet m "margin" b
margin: ${1:0 20px};$0
endsnippet

snippet mb "margin-bottom" b
margin-bottom: ${1:0};$0
endsnippet

snippet mt "margin-top" b
